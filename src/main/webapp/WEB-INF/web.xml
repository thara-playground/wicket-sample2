<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

  <display-name>sample</display-name>

  <filter>
    <filter-name>wicket</filter-name>
    <filter-class>org.apache.wicket.protocol.http.WicketFilter</filter-class>
    <init-param>
      <param-name>applicationClassName</param-name>
      <param-value>org.tomochika1985.sample.WicketApplication</param-value>
    </init-param>
    <init-param>
      <param-name>twitter-consumer-key</param-name>
      <param-value>XjugEtdqq8bwlgKziU7acw</param-value>
    </init-param>
    <init-param>
      <param-name>twitter-consumer-secret</param-name>
      <param-value>lpTsKZxdJJr8HEUywY4YDOq3BzaWvcXQmEcdKL4AHk</param-value>
    </init-param>
  </filter>

  <filter-mapping>
    <filter-name>wicket</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>
